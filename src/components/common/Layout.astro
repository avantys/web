---
import BaseLayout from '@/layouts/BaseLayout.astro';
interface Props {
  title: string;
  description: string;
  canonical?: string;
  noindex?: boolean;
  ogImage?: string;
  twitterCard?: string;
}
const props = Astro.props as Props;
---
<BaseLayout {...props}>
  <Fragment slot="nav">
    <slot name="nav" />
  </Fragment>
  <slot />
</BaseLayout>
